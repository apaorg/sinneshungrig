{
    "name": "Lonch2",
    "builds": [
        {
            "src": "client/next.config.js",
            "use": "@now/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "client/$1",
            "continue": true
        }
    ],
    "env": {
        "MONGODB_URI": "@my-mongodb-uri"
    },
    "build": {
        "env": {
            "MONGODB_URI": "@my-mongodb-uri"
        }
    }
}
